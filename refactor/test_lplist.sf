{-#
    INCLUDE "lplist.sf"
#-}

let testFoldr : TestFn = {
    name = "foldr",
    fn   = \(__ : Unit) -> {
        assertEq[Int]
            intEq
            intToString
            (lFoldr[Int,Int] (\(a : Int) (b : Int) -> a + b) 0 (take[Int] 5 (nats 1)))
            15;
        True
    }
};

let testFoldl : TestFn = {
    name = "foldl",
    fn   = \(__ : Unit) -> {
        assertEq[Int]
            intEq
            intToString
            (lFoldl[Int,Int] (\(b : Int) (a : Int) -> a + b) 0 (take[Int] 5 (nats 1)))
            15;
        True
    }
};
